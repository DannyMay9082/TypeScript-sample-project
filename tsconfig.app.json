{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "paths": {
      "~assets/*": ["src/assets/*"],
      "~core/*": ["src/app/core/*"],
      "~environments/*": ["src/environments/*"],
      "~shared/*": ["src/app/shared/*"],
      "~modules/*": ["src/app/modules/*"],
      "~app/*": ["src/app/*"]
    }
  },
  "files": ["src/main.ts", "src/polyfills.ts"],
  "include": ["src/**/*.d.ts"]
}
