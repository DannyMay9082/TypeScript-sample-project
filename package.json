{
  "name": "angular-example-app",
  "version": "7.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve --open",
    "http-server": "http-server dist/ -c-1",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:library": "ng build ngx-example-library",
    "test": "ng test",
    "test:app:watch": "ng test angular-example-app --code-coverage --watch=true",
    "test:library:watch": "ng test ngx-example-library --code-coverage --watch=true",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ci": "npm run lint && npm run test && npm run e2e && npm run build:prod",
    "extract": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/*.json --sort --format namespaced-json --format-indentation '  ' --marker _",
    "translate": "node ./scripts/translate.js",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && git push --follow-tags origin master",
    "docker": "docker build -t angularexampleapp . && docker run -d -p 4200:80 angularexampleapp",
    "update": "ng update --all --force",
    "deploy": "bash scripts/deploy.sh"
  },
  "private": true,
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.0.0"
  },
  "dependencies": {
    "@angular/animations": "7.2.7",
    "@angular/common": "7.2.7",
    "@angular/compiler": "7.2.7",
    "@angular/core": "7.2.7",
    "@angular/fire": "5.1.1",
    "@angular/flex-layout": "7.0.0-beta.23",
    "@angular/forms": "7.2.7",
    "@angular/http": "7.2.7",
    "@angular/platform-browser": "7.2.7",
    "@angular/platform-browser-dynamic": "7.2.7",
    "@angular/router": "7.2.7",
    "@ismaestro/ngx-scroll-to-first-invalid": "2.0.0",
    "@sentry/browser": "4.6.4",
    "classlist.js": "1.1.20150312",
    "core-js": "2.6.5",
    "firebase": "5.8.4",
    "rxjs": "6.4.0",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.3",
    "@angular-devkit/build-ng-packagr": "0.13.4",
    "@angular/cdk": "7.3.3",
    "@angular/cli": "7.3.3",
    "@angular/compiler-cli": "7.2.7",
    "@angular/language-service": "7.2.7",
    "@angular/material": "7.3.3",
    "@angular/platform-server": "7.2.7",
    "@angular/service-worker": "7.2.7",
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@ismaestro/ngx-example-library": "1.0.0",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/html2canvas": "0.0.35",
    "@types/jasmine": "3.3.9",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "11.9.5",
    "axios": "0.18.0",
    "bowser": "2.1.0",
    "codelyzer": "4.5.0",
    "coveralls": "3.0.3",
    "deepmerge": "3.2.0",
    "fs": "0.0.1-security",
    "hammerjs": "2.0.8",
    "html2canvas": "1.0.0-alpha.12",
    "http-server": "0.11.1",
