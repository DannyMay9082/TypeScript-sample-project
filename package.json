    "karma-remap-istanbul": "0.6.0",
    "karma-scss-preprocessor": "4.0.0",
    "moment": "2.24.0",
    "ng-bullet": "^1.0.3",
    "ng-packagr": "4.7.1",
    "ngx-store": "2.1.0",
    "node-sass": "4.11.0",
    "path": "0.12.7",
    "protractor": "5.4.2",
    "standard-version": "5.0.1",
    "ts-helpers": "1.1.2",
    "ts-node": "8.0.2",
    "tsickle": "0.34.3",
